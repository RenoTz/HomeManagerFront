{
  "name": "ng6",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build --extract-css",
    "build:aot": "npm run build:aot:prod",
    "build:aot:dev": "cross-env BUILD_AOT=1 npm run build:dev",
    "ci:travis": "cross-env BUILD_E2E=1 npm run lint && npm run test:ci && npm run build:aot && npm run e2e:travis",
    "e2e": "ng e2e",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "test": "ng test --code-coverage",
    "test:ci": "karma start --single-run --browsers ChromeTravisCi",
    "tslint": "tslint \"src/**/*.ts\" --project tsconfig.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "font-awesome": "4.7.0",
    "npm-font-open-sans": "1.1.0",
    "rxjs": "~6.2.0",
    "source-sans-pro": "2.45.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "cross-env": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-dev-server": "^3.1.14"
  }
}
